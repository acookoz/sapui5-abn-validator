{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "mmd.abnValidator",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "mmd.abnValidator.view.ABNEntryView",
			"type": "XML",
			"id": "app"
		},
		"autoPrefixId": true,
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "1.34",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.34.0"
				},
				"sap.m": {
					"minVersion": "1.34.0"
				},
				"sap.ui.layout": {
					"minVersion": "1.34.0"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "mmd.abnValidator.i18n.i18n"
				}
			}
		},
		"contentDensities": {
			"compact": false,
			"cozy": false
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": "customcss"
			}]
		}
	}
}